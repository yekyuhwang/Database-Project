doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Document
    body 
        each data in list 
            h1 #{data.name}
            h4 #{data.gender}
            h4 #{data.mobile}
            h4 #{data.birth}
            hr