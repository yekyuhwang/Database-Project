doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Document
    body 
        h1 Hello World
        each data in list
            h2 #{data.name}
            h4 #{data.mobile}
            h4 #{data.post_code}
            h4 #{data.address}
            h4 #{data.detail_address}
            h4 #{data.birth}
            hr