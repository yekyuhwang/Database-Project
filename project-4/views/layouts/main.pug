include ./header
include ./footer
include ../mixins/subBanner

doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Document
        link(rel="stylesheet", href="/css/styles.css")
        //- / -> 루트페스
        //- ./ => 현재페스
        //- ../ => 상위페스
    body 
        header.header
            +header

        main.main
            +subBanner 
            block content
            
        footer.footer
            +footer     